{
  "name": "easy-mock-api-template",
  "version": "1.0.1",
  "description": "基于 axios 的接口定义模板",
  "main": "index.js",
  "scripts": {
    "create-api": "easymock init dist",
    "build": "rimraf dist && webpack --config example/webpack.conf.js --progress --hide-modules",
    "server": "serve ./dist -p 3333",
    "dev": "npm run create-api && npm run build"
  },
  "author": "Chuangker",
  "license": "MIT",
  "keywords": [
    "easy-mock",
    "mock"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easy-mock-templates/axios.git"
  },
  "devDependencies": {
    "axios": "^0.16.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "easy-mock-cli": "^0.4.3",
    "html-webpack-plugin": "^2.28.0",
    "rimraf": "^2.6.1",
    "serve": "^5.1.5",
    "webpack": "^2.5.1"
  }
}
